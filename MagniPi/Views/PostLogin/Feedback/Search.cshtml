
<div class="content-wrapper">

    <div class="container-fluid">

        @*@{Html.RenderPartial("_ServerMessage");}*@

        <!-- Content Header (Page header) -->

        <section class="content-header">
            <h1>
                Feedback
            </h1>
            <ol class="breadcrumb">
                <li>Admin</li>
                <li class="active">Feedback</li>
            </ol>
        </section>

        <form id="frmFeedbackListing">
            <section class="content">
                <div class="row">
                    <div id="divScrollContainer" class="col-md-12">
                        <h2 id="" class="page-header">Feedback Listing</h2>

                        <div class="box box-default">
                            <div class="box-body">

                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="txtName" style="color: #333" class="">Event</label>
                                            <input type="text" class="form-control input-sm" name="" id="txtName" value="" />
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="txtName" style="color: #333" class="">Customer</label>
                                            <input type="text" class="form-control input-sm" name="" id="txtName" value="" />
                                        </div>
                                    </div>
                                </div>

                                <div class="box-footer">
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="btn-group">
                                                <button type="button" id="btnSearch" class="btn btn-instagram btn-sm">Search</button>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="btn-group pull-right">
                                                <a href="@Url.Action("Details", "Feedback")" type="button" id="btnView" class="btn btn-instagram btn-sm">View</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="box box-solid">

                                            <div class="box-body table-responsive no-padding mdm-table">
                                                <table class="table table-striped" id="tblFeedback">
                                                    <tr id="subhead">
                                                        <th></th>
                                                        <th>Event Name</th>
                                                        <th>Customer Name</th>
                                                        <th>Feedback</th>
                                                        <th>Date</th>
                                                    </tr>
                                                    <tr id="subhead">
                                                        <th><input type="radio" name="r1" id="" class='' /></th>
                                                        <td>Pokemon Scrap-2016 Event</td>
                                                        <td>Sanchita Sawant</td>
                                                        <td>Details have been posted on the official site about a scheme called...</td>
                                                        <td>01/08/2016</td>
                                                    </tr>

                                                </table>
                                            </div>

                                            <!-- /.box-body -->
                                            <div class="box-footer clearfix">
                                                <div id="divSearchGridOverlay" class="overlay">
                                                    <i class="fa fa-refresh fa-spin"></i>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- /.box -->
                                    </div>
                                    <!-- /.col (left) -->
                                </div>

                            </div>
                        </div>

                    </div>
                </div>
            </section>
        </form>

    </div>
</div>